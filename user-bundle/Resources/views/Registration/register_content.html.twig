<section class="breadcumb-area bg-img bg-overlay" style="background-image: url({{ asset('FrontOffice/img/3.jpg')}})">
    <div class="bradcumbContent">
        <p>See whatâ€™s new</p>
        <h2>Register</h2>
    </div>
</section>



<section class="login-area section-padding-100">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-12 col-lg-8">
                <div class="login-content">
                    <h3>Welcome Back</h3>
                    <!-- Login Form -->
                    <div class="login-form">

                        {% trans_default_domain 'FOSUserBundle' %}

                        {{ form_start(form, {'method': 'post', 'action': path('fos_user_registration_register'), 'attr': {'class': 'fos_user_registration_register'}}) }}
                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label for="inputEmail4">nom</label>
                                {{ form_widget(form.nom, { 'attr': {'class': 'form-control'} }) }}
                            </div>
                            <div class="form-group col-md-6">
                                <label for="inputPassword4">prenom</label>
                                {{ form_widget(form.prenom, { 'attr': {'class': 'form-control'} }) }}
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="inputAddress">Email</label>
                            {{ form_widget(form.email, { 'attr': {'class': 'form-control'} }) }}
                        </div>

                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label for="inputEmail4">Password</label>
                                {{ form_widget(form.plainPassword.first, { 'attr': {'class': 'form-control'} }) }}
                            </div>
                            <div class="form-group col-md-6">
                                <label for="inputPassword4">Confirm password</label>
                                {{ form_widget(form.plainPassword.second, { 'attr': {'class': 'form-control'} }) }}
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label for="inputCity">Username</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text" id="inputGroupPrepend2">@</span>
                                    </div>
                                    {{ form_widget(form.username, { 'attr': {'class': 'form-control'} }) }}
                                </div>
                            </div>
                            <div class="form-group col-md-3">
                                <label for="inputState">date_naissance</label>
                                {{ form_widget(form.date_naissance, { 'attr': {'class': 'form-control'} }) }}
                            </div>
                            <div class="form-group col-md-3">
                                <label for="inputZip">Sexe</label>
                                {{ form_widget(form.sexe, { 'attr': {'class': 'form-control'} }) }}

                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label for="inputCity">num_tel</label>
                                <div class="input-group">
                                    {{ form_widget(form.num_tel, { 'attr': {'class': 'form-control'} }) }}
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="inputCity">cin</label>
                                <div class="input-group">
                                    {{ form_widget(form.cin, { 'attr': {'class': 'form-control'} }) }}
                                </div>
                            </div>
                        </div>

                        <div class="form-row">
                            <label for="inputZip">Picture</label>
                            <br>
                            {{ form_widget(form.photo) }}

                        </div>
                        <div class="form-row" style="margin-top: 24px;">
                            {{ form_widget(form.valider, { 'attr': {'class': 'btn btn-dark'} }) }}

                        </div>
                    </div>
                    {{ form_end(form) }}

                </div>
            </div>
        </div>
    </div>
    </div>
</section>




